{
  "name": "yt-scraper",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "grpc-tools": "^1.13.1",
    "protobufjs": "^8.0.0",
    "ts-proto": "^2.10.1"
  },
  "scripts": {
    "start": "node dist/main.js",
    "start:bun": "bun src/main.ts",
    "dev": "bun --watch src/main.ts",
    "build": "tsc && tsc-alias",
    "protoc:generate": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/gen --ts_proto_opt=outputServices=grpc-js,esModuleInterop=true,importSuffix=.js -I ../../ proto/common.proto proto/yt_scraper.proto"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "google-protobuf": "^4.0.1",
    "youtubei.js": "^16.0.1",
    "zod": "^4.3.4"
  }
}
